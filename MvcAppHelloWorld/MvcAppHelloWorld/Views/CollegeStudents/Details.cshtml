@model BusinessObjectModel.College
@{
    ViewBag.Title = "College Student Details";
}
<h2>Edit College Students</h2>
@using (Html.BeginForm("EditStudentDetails", "CollegeStudents", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <br />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.ID)
        @Html.Partial("_Edit", Model)
        <div class="form-group">
            @Html.LabelFor(model => model.College_Name, htmlAttributes: new { @class = "control-label col-md-4" })
            <div class="col-md-8">
                @Html.EditorFor(model => model.College_Name, ViewBag.readOnly == true ? (object)new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } } : (object)new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.College_Name, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Generation_of_Student, htmlAttributes: new { @class = "control-label col-md-4" })
            <div class="col-md-8">
                @Html.EditorFor(model => model.Generation_of_Student, ViewBag.readOnly == true ? (object)new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } } : (object)new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Generation_of_Student, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" @ViewBag.disabled />
            </div>
        </div>
    </div>
}
<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID }, new { @class = "btn btn-primary btn-sm" })
    @Html.ActionLink("Back to List", "Index", "College", new { @class = "btn btn-primary btn-sm" })
    @Html.ActionLink("Download Details", "Export", new { id = Model.ID }, new { @class = "btn btn-primary btn-sm" })
    <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#exampleModal">
        Delete
    </button>
</div>
<!-- Modal -->
@Html.Partial("_Modal", Model)
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")  
    @Scripts.Render("~/Scripts/main.js")
}